    "electron": "node_modules/electron/dist/electron",
    "serve": "ng serve",
    "build": "ng build --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "start:electron": "ng build --base-href ./ && node_modules/electron/dist/electron dist/app.js",
